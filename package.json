{
  "name": "agenda-tcc-instituicao",
  "version": "1.0.0",
  "description": "Sistema integrado de gestão acadêmica com MySQL e Excel",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "debug": "node --inspect server.js",
    "test": "jest",
    "test:unit": "jest test/unit",
    "test:frontend": "jest test/frontend",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "setup": "mkdir logs && mkdir uploads"
  },
  "keywords": [
    "academico",
    "gestao",
    "mysql",
    "nodejs",
    "agenda",
    "excel",
    "importador"
  ],
  "author": "Vinicius <seu-email@dominio.com>",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-fileupload": "^1.5.1",
    "express-session": "^1.18.1",
    "fullcalendar": "^6.1.17",
    "fullcalendar-scheduler": "^6.1.17",
    "html2canvas": "^1.4.1",
    "jspdf": "^3.0.1",
    "multer": "^1.4.5-lts.2",
    "mysql": "^2.18.1",
    "mysql2": "^3.6.0",
    "winston": "^3.11.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "jest": "^29.7.0",
    "jsdom": "^26.1.0",
    "nodemon": "^3.0.2",
    "jest-environment-jsdom": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "moduleNameMapper": {
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/tests/"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  }
}